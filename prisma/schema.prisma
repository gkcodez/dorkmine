generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Dork {
  id            String    @id @default(uuid())
  title         String    @unique
  description   String?
  value         String
  author        String?
  favorite      Boolean   @default(false)
  createdAt     DateTime  @default(now())
}
